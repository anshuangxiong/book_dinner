  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>com.anxiong.bookdinner.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Admin2Servlet</servlet-name>
